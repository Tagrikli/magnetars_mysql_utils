CREATE TABLE `magnetars`.`event_database` (
    `id` INT NOT NULL AUTO_INCREMENT,
    `method` VARCHAR(20) NOT NULL,
    `mode` VARCHAR(20) NOT NULL,
    `filename` TEXT NOT NULL,
    `trigtime` FLOAT NOT NULL,
    `trigutc` DATETIME NOT NULL,
    `duration` FLOAT NOT NULL,
    `hr_25` FLOAT NOT NULL,
    `hr_50` FLOAT NOT NULL,
    `hr_75` FLOAT NOT NULL,
    `event_grade` TINYINT NOT NULL,
    `l_shell` FLOAT NOT NULL,
    `latitude` FLOAT NOT NULL,
    `longitude` FLOAT NOT NULL,
    `ratio` FLOAT NOT NULL,
    `saa` FLOAT NOT NULL,
    `sgr` FLOAT NOT NULL,
    `sgrb` FLOAT NOT NULL,
    `lgrb` FLOAT NOT NULL,
    `sflare` FLOAT NOT NULL,
    `tgf` FLOAT NOT NULL,
    `transnt` FLOAT NOT NULL,
    `distpar` FLOAT NOT NULL,
    `loclpar` FLOAT NOT NULL,
    `no_evaluation` FLOAT NOT NULL,
    `sig_1` FLOAT NULL,
    `sig_2` FLOAT NULL,
    `sig_3` FLOAT NULL,
    `sig_4` FLOAT NULL,
    `sig_5` FLOAT NULL,
    `sig_6` FLOAT NULL,
    `sig_7` FLOAT NULL,
    `sig_8` FLOAT NULL,
    `sig_9` FLOAT NULL,
    `sig_10` FLOAT NULL,
    `sig_11` FLOAT NULL,
    `sig_12` FLOAT NULL,
    `trig_det_1` BOOLEAN NULL,
    `trig_det_2` BOOLEAN NULL,
    `trig_det_3` BOOLEAN NULL,
    `trig_det_4` BOOLEAN NULL,
    `trig_det_5` BOOLEAN NULL,
    `trig_det_6` BOOLEAN NULL,
    `trig_det_7` BOOLEAN NULL,
    `trig_det_8` BOOLEAN NULL,
    `trig_det_9` BOOLEAN NULL,
    `trig_det_10` BOOLEAN NULL,
    `trig_det_11` BOOLEAN NULL,
    `trig_det_12` BOOLEAN NULL,
    PRIMARY KEY (`id`)
) ENGINE = InnoDB;